<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Shopping.DemoApp.UWP.Themes">

    <Color x:Key="ShoppingDemoAppDarkBlueColor">#011F4F</Color>
    <SolidColorBrush x:Key="ShoppingDemoAppDarkBlueColorBrush"
                     Color="{StaticResource ShoppingDemoAppDarkBlueColor}" />

    <Color x:Key="ShoppingDemoAppLigthCyanColor">#7394C8</Color>
    <SolidColorBrush x:Key="ShoppingDemoAppLigthCyanColorBrush"
                     Color="{StaticResource ShoppingDemoAppLigthCyanColor}" />


    <Color x:Key="ShoppingDemoAppPinkColor">#E1118C</Color>
    <SolidColorBrush x:Key="ShoppingDemoAppPinkColorBrush"
                     Color="{StaticResource ShoppingDemoAppPinkColor}" />

    <Color x:Key="ShoppingDemoAppDarkPinkColor">#a30e66</Color>
    <SolidColorBrush x:Key="ShoppingDemoAppDarkPinkColorBrush"
                     Color="{StaticResource ShoppingDemoAppDarkPinkColor}" />
    
    <SolidColorBrush x:Key="ShoppingDemoAppWhiteBrush"
                     Color="White" />
    
    <SolidColorBrush x:Key="ApplicationPageBackgroundThemeBrush"
                     Color="#FFFFFFFF" />

    <SolidColorBrush x:Key="HomeBackgroundColorBrush"
                     Color="#FFF2F4F6" />

</ResourceDictionary>
